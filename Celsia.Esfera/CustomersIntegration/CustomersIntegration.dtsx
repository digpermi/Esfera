                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
  SU.NIC,
  TP.DESC_TIPO || ' ' || CLL.NOM_CALLE || ' '  || CL.DUPLICADOR || ' '  || CL.NUM_PUERTA DIRECCION,
  DECODE(TP3.TIP_SERV, 'TE', 'CE', TP3.TIP_SERV) AS TIPO_IDENTIFICACION,
  CL.DOC_ID as "# DE IDENTIFICACIÓN",
  CL.NOM_CLI AS NOMBRE,
  CL.APE1_CLI AS APELLIDO1,
  CL.TFNO_CLI TELEFONO,
  CL.USR_VARCHAR2 CORREO_ELECTRONICO,
  CL.F_ACTUAL as fecha_modificación
FROM SUMCON SU
JOIN CLIENTES CL ON (SU.COD_CLI = CL.COD_CLI)
JOIN MTARIFAS MT ON (MT.COD_TAR = SU.COD_TAR)
JOIN CALLEJERO CLL ON (CL.COD_CALLE = CLL.COD_CALLE)
JOIN LOCALIDADES LO ON (CLL.COD_LOCAL = LO.COD_LOCAL)
JOIN TIPOS TP ON (TP.TIPO = CLL.TIP_VIA)
JOIN TIPOS TP2 ON (TP2.TIPO = SU.TIP_PER_FACT)
JOIN DEPTOS DPT ON (CLL.COD_DEPTO =  DPT.COD_DEPTO AND CLL.COD_PROV = DPT.COD_PROV)
JOIN TIPOS TP3 ON (TP3.TIPO = CL.TIP_DOC)
JOIN ESTADOS EST ON (EST.ESTADO = SU.EST_SUM)
WHERE SU.TIP_SERV IN ('SV100','SV120')
AND SU.EST_SUM = EST.ESTADO
AND SU.NIC &gt; 0
GROUP BY SU.NIC,SU.COD_CLI,CL.NOM_CLI,CL.TFNO_CLI,
CL.USR_VARCHAR2,MT.DESC_TAR,CL.APE1_CLI,CL.APE2_CLI,LO.NOM_LOCAL,CLL.COD_UNICOM,
CL.NUM_PUERTA,TP.DESC_TIPO,CLL.NOM_CALLE,CL.DUPLICADOR,CLL.COD_PROV,CLL.COD_DEPTO,DPT.NOM_DEPTO,
TP2.DESC_TIPO,CL.COD_CLI, TP3.TIP_SERV,EST.DESC_EST,CL.DOC_ID,CL.F_ALTA, CL.F_ACTUAL</property>
